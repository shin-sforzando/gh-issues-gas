{
  "name": "gh-issues-gas",
  "version": "1.0.0",
  "description": "GitHub Issues Importer from Google Spreadsheet",
  "main": "index.js",
  "scripts": {
    "lint": "tslint --fix src/**/*.ts",
    "test": "jest",
    "deploy": "clasp push"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/shin-sforzando/gh-issues-gas.git"
  },
  "keywords": [
    "github",
    "google-apps-script",
    "clasp",
    "jest"
  ],
  "author": "sforzando LLC.",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/shin-sforzando/gh-issues-gas/issues"
  },
  "homepage": "https://github.com/shin-sforzando/gh-issues-gas#readme",
  "dependencies": {
    "@google/clasp": "^2.3.0",
    "@types/google-apps-script": "1.0.14",
    "@types/jest": "^26.0.7",
    "jest": "^26.1.0",
    "prettier": "^2.0.5",
    "ts-jest": "^26.5.6",
    "tslint": "^6.1.2",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.3.0",
    "typescript": "^3.9.7"
  },
  "jest": {
    "verbose": true,
    "roots": [
      "<rootDir>/src"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.tsx?$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
